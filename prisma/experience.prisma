model Experience {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  companyName String
  location    String
  position    String
  startDate   DateTime
  endDate     DateTime?
  works       String[]
  techs       String[]

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("experiences")
}
